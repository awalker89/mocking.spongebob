mocking_spongebob <- function(x) {
y <- strsplit(x, split="")
y <- unlist(y)
y <- data.frame(x=y)
y$flag <- sample(1:2, nrow(y), replace=T)
y$x_new <- NA
y[y$flag==1,]$x_new <- tolower(y[y$flag==1,]$x)
y[y$flag==2,]$x_new <- toupper(y[y$flag==2,]$x)
paste0(y$x_new, collapse="")
}
mocking_spongebob("mocking spongebob")
#' @title mocking_spongebob
#' @name mocking_spongebob
#' @author Jennifer An
#' @
#' @export
mocking_spongebob <- function(x) {
y <- strsplit(x, split="")
y <- unlist(y)
y <- data.frame(x=y)
y$flag <- sample(1:2, nrow(y), replace=T)
y$x_new <- NA
y[y$flag==1,]$x_new <- tolower(y[y$flag==1,]$x)
y[y$flag==2,]$x_new <- toupper(y[y$flag==2,]$x)
return(paste0(y$x_new, collapse=""))
}
mocking_spongebob("mocking spongebob")
library(mocking.spongebob)
mocking.spongebob::mocking_spongebob("hi")
